@using CRM.Services.Data.ViewModels.Users.Contracts

@{
    var data = ViewData["TypeOfCompanies"];
    var typeOfCompanies = new List<DatabaseDataDropdownViewModel>();
    if (data != null)
    {
        var typeOfCompaniesVm = (List<DatabaseDataDropdownViewModel>)data;
        typeOfCompanies.AddRange(typeOfCompaniesVm);
    }
}

@(Html.Kendo().DropDownListFor(m => m)
        .DataTextField("Text")
        .DataValueField("Value")
         .BindTo(typeOfCompanies)
)
