@using CRM.Services.Data.ViewModels.Users.Contracts

@{
    var data = ViewData["Providers"];
    var providers = new List<DatabaseDataDropdownViewModel>();
    if (data != null)
    {
        var providersVm = (List<DatabaseDataDropdownViewModel>)data;
        providers.AddRange(providersVm);
    }
}

@(Html.Kendo().DropDownListFor(m => m)
        .DataTextField("Text")
        .DataValueField("Value")
          .BindTo(providers)
)
